{
  "name": "interactive-comments-section",
  "private": true,
  "version": "0.0.0",
  "homepage": "https://bohdancho.github.io/interactive-comments-section/",
  "type": "module",
  "scripts": {
    "dev": "concurrently 'bun:dev:frontend' 'bun:dev:backend'",
    "dev:frontend": "vite",
    "dev:backend": "NODE_ENV=development bun run server/index.ts",
    "prebuild": "rimraf dist",
    "build": "tsc && vite build",
    "start": "NODE_ENV=production nodemon",
    "preview": "concurrently bun:preview:frontend bun:start",
    "preview:frontend": "vite preview"
  },
  "dependencies": {
    "@tanstack/react-query": "^4.33.0",
    "@trpc/client": "^10.38.1",
    "@trpc/react-query": "^10.38.1",
    "bun-types": "^1.0.1",
    "concurrently": "^8.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@prisma/client": "^5.2.0",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@trpc/server": "^10.38.1",
    "@types/express": "^4.17.17",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import-alias": "^1.2.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "express": "^4.18.2",
    "postcss": "^8.4.21",
    "prettier": "^3.0.2",
    "prettier-plugin-organize-imports": "^3.2.3",
    "prettier-plugin-tailwindcss": "^0.5.3",
    "prisma": "^5.2.0",
    "react-inlinesvg": "^3.0.1",
    "rimraf": "^5.0.1",
    "tailwindcss": "^3.2.4",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.2",
    "vite": "^4.0.0",
    "vite-plugin-eslint": "^1.8.1",
    "zod": "^3.22.2"
  }
